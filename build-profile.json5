{
  // app的构建配置
  "app": {
    //签名配置
    "signingConfigs": [
    ],
    //产品配置
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compileSdkVersion": "4.1.0(11)",
        "compatibleSdkVersion": "4.0.0(10)",
        "runtimeOS": "HarmonyOS",
      }
    ],
    //构建类型集合
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  //组件集合
  "modules": [
    // app组件
    {
      // 组件名称
      "name": "app",
      // 组件源码位置
      "srcPath": "./app",
      // 构建目标
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    //shareLib组件
    {
      "name": "common_ui",
      "srcPath": "./common_ui",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    }
  ]
}